{
    "name": "vscode",
    "author": "pjreiniger",
    "displayName": "vscode",
    "description": "SnobotSim setup utility",
    "repository": "https://github.com/wpilibsuite/vscode-wpilib",
    "homepage": "https://github.com/wpilibsuite/vscode-wpilib",
    "version": "0.1.0",
    "publisher": "pjreiniger",
    "license": "MIT",
    "icon": "snobotsim-128.png",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:.wpilib/wpilib_preferences.json",
        "workspaceContains:build/vscodeconfig.json",
        "onCommand:snobotsim.setupProject"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "snobotsim.setupProject",
                "title": "%snobotsim.setupProject.title%",
                "category": "SnobotSim"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "when": "resourceFilename == build.gradle",
                    "command": "snobotsim.setupProject",
                    "group": "SnobotSim"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run lint && webpack --mode production",
        "clean": "rmdir-cli out",
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "webpack": "webpack --mode development",
        "webpack-watch": "webpack --mode development --watch",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "unittest": "mocha -r ts-node/register -u tdd src/unittest/*.ts",
        "test": "npm run webpack && node ./node_modules/vscode/bin/test",
        "vscePackage": "vsce package"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mkdirp": "^0.5.2",
        "@types/mocha": "^5.2.7",
        "@types/ncp": "^2.0.1",
        "@types/node-fetch": "^2.5.4",
        "@types/triple-beam": "^1.3.0",
        "@types/xml2js": "^0.4.5",
        "copy-webpack-plugin": "^5.0.3",
        "del": "^5.1.0",
        "event-stream": "=4.0.1",
        "gulp": "^4.0.0",
        "gulp-filter": "^6.0.0",
        "gulp-json-transform": "^0.4.7",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-typescript": "^5.0.0-alpha.3",
        "mocha": "^6.2.0",
        "rmdir-cli": "^2.0.6",
        "run-sequence": "^2.2.1",
        "ts-loader": "^6.0.4",
        "ts-node": "^8.3.0",
        "tslint": "^5.18.0",
        "typescript": "^3.7.4",
        "vsce": "^1.66.0",
        "vscode": "^1.1.33",
        "vscode-nls-dev": "^3.3.1",
        "webpack": "^4.38.0",
        "webpack-cli": "^3.3.10"
    },
    "dependencies": {
        "glob": "^7.1.4",
        "java-properties": "^1.0.2",
        "jsonc-parser": "^2.2.0",
        "logform": "^2.1.2",
        "mkdirp": "^0.5.1",
        "ncp": "^2.0.0",
        "node-fetch": "^2.3.0",
        "npm": "^6.13.6",
        "triple-beam": "^1.3.0",
        "vscode-cpptools": "3.1.0",
        "vscode-jsonrpc": "^4.0.0",
        "vscode-nls": "^4.1.1",
        "vscode-wpilibapi": "^2019.3.1",
        "winston": "^3.2.1",
        "winston-transport": "^4.3.0",
        "wpilib-riolog": "^0.2.0",
        "xml2js": "^0.4.23"
    }
}
