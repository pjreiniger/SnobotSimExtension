{
    "name": "vscode",
    "author": "pjreiniger",
    "displayName": "vscode",
    "description": "SnobotSim setup utility",
    "repository": "https://github.com/wpilibsuite/vscode-wpilib",
    "homepage": "https://github.com/wpilibsuite/vscode-wpilib",
    "version": "0.1.0",
    "publisher": "pjreiniger",
    "license": "MIT",
    "icon": "wpilib-128.png",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:snobotsim.setupProject"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "snobotsim.setupProject",
                "title": "%snobotsim.setupProject.title%",
                "category": "SnobotSim"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "when": "resourceFilename == build.gradle",
                    "command": "snobotsim.setupProject",
                    "group": "SnobotSim"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run lint  && webpack --mode production",
        "clean": "rmdir-cli out",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "webpack": "webpack --mode development",
        "webpack-watch": "webpack --mode development --watch",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "unittest": "mocha -r ts-node/register -u tdd src/unittest/*.ts",
        "test": "npm run webpack && node ./node_modules/vscode/bin/test",
        "vscePackage": "vsce package"
    },
    "devDependencies": {
        "@types/mkdirp": "^0.5.2",
        "@types/mocha": "^5.2.5",
        "@types/node": "^8.9.5",
        "@types/ncp": "^2.0.1",
        "typescript": "^3.3.1",
        "tslint": "^5.11.0",
        "vscode": "^1.1.33",
        "webpack": "^4.25.1",
        "webpack-cli": "^3.1.2"
    },
    "dependencies": {
        "mkdirp": "^0.5.1",
        "ncp": "^2.0.0",
        "webpack": "^4.25.1",
        "webpack-cli": "^3.1.2"
    }
}
