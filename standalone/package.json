{
  "name": "standalone",
  "author": "pjreiniger",
  "description": "SnobotSim setup utility",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "compile": "tsc -p ./",
    "clean": "rmdir-cli out",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run compile && node_modules\\electron\\dist\\electron.exe .",
    "watch": "tsc -watch -p ./",
    "packageWindows": "electron-packager . snobotsim_utility --platform=win32 --arch=ia32 --overwrite --out ./build",
    "packageLinux": "electron-packager . snobotsim_utility --platform=linux --overwrite --out ./build",
    "packageMac": "electron-packager . snobotsim_utility --platform=darwin --overwrite --out ./build"
  },
  "main": "out/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pjreiniger/SnobotSimExtension"
  },
  "keywords": [],
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mkdirp": "^0.5.2",
    "@types/ncp": "^2.0.1",
    "@types/temp": "^0.8.32",
    "@types/triple-beam": "^1.3.0",
    "@types/node-fetch": "^2.5.0",
    "electron": "^5.0.8",
    "electron-packager": "^14.0.3",
    "rmdir-cli": "^2.0.6",
    "tslint": "^5.19.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "glob": "^7.1.3",
    "java-properties": "^1.0.2",
    "jsonc-parser": "^2.0.2",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "temp": "^0.9.0",
    "triple-beam": "^1.3.0",
    "node-fetch": "^2.3.0",
    "winston": "^3.1.0",
    "wpilib-riolog": "^0.2.0"
  }
}
